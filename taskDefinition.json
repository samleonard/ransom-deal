{
  "containerDefinitions": [
    {
      "name": "ransom-deal",
      "image": "952283372302.dkr.ecr.us-west-2.amazonaws.com/ransom-deal:137bb54b2603ea0663b2b36f255f30ba1cae3916",
      "cpu": 0,
      "portMappings": [
        {
          "name": "ransom-deal-80-tcp",
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "essential": true,
      "environment": [],
      "mountPoints": [],
      "volumesFrom": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-create-group": "true",
          "awslogs-group": "/ecs/",
          "awslogs-region": "us-west-2",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "healthCheck": {
        "command": ["CMD-SHELL", "curl -f http://localhost/ || exit 1"],
        "interval": 30,
        "timeout": 5,
        "retries": 3
      }
    }
  ],
  "family": "ransom-deal",
  "taskRoleArn": "arn:aws:iam::952283372302:role/ecsTaskExecutionRole",
  "executionRoleArn": "arn:aws:iam::952283372302:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "volumes": [],
  "placementConstraints": [],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "1024",
  "memory": "3072",
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  },
  "tags": []
}
